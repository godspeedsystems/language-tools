{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "outDir": "out",
    "rootDir": "src",
    "skipLibCheck": true,
    "composite": true
  },
  "include": ["src"],
	"exclude": ["node_modules"]
}
