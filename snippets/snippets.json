{
  "create godspeed event": {
    "prefix": ["godspeed event"],
    "body": [
      "/${1:event-endpoint}.${2|http,kafka,salesforce,cron|}.${3|get,put,post,delete,options,insert(),update(),delate(),upsert(),undelete(),timezone|}:",
      "\tauthn: false",
      "\tsummary: ${4:'summary of the event'}",
      "\tdescription: ${5:'description of the event'}",
      "\tfn: ${6:'full path of the function, seprated with dots, remove .yaml, like: com.biz.helloworld'}",
      "\ton_validation_error: com.jfs.handle_validation_error",
      "\tparams:${7}",
      "\tbody:${8}",
      "\tresponses:${9}",
      "\t\t'200':",
      "\t\t\tdescription: OK",
      "\t\t\tcontent:",
      "\t\t\t\tapplication/json:",
      "\t\t\t\t\tschema:",
      "\t\t\t\t\t\ttype: object${0}",
      "\n"
    ]
  },
  "create godspeed workflow": {
    "prefix": ["godspeed workflow"],
    "body": [
      "summary: ${1:'the title'}",
      "description: ${2:'more details'}",
      "id: ${3:'unique ID, for better logging visibility'}",
      "tasks:",
      "\t- id: ${4:'unique id of the task'}",
      "\t\tfn: ${5|com.gs.return,com.gs.http,com.gs.kafka,com.gs.datastore,com.gs.elasticgraph,com.gs.transform,com.gs.series,com.gs.parallel,com.gs.switch,com.gs.each_sequential,com.gs.each_parallel,com.gs.log,com.gs.dynamic_fn,com.gs.aws,com.gs.redis|}",
      "\t\ton_error:",
      "\t\t\tcontinue: false",
      "\n"
    ]
  },
  "create godspeed datasource": {
    "prefix": ["godspeed datasource"],
    "body": [
      "summary: ${1:'the title'}",
      "description: ${2:'more details'}",
      "id: ${3:'unique ID, for better logging visibility'}",
      "tasks:",
      "\t- id: ${4:'unique id of the task'}",
      "\t\tfn: ${5|com.gs.return,com.gs.http,com.gs.kafka,com.gs.datastore,com.gs.elasticgraph,com.gs.transform,com.gs.series,com.gs.parallel,com.gs.switch,com.gs.each_sequential,com.gs.each_parallel,com.gs.log,com.gs.dynamic_fn,com.gs.aws,com.gs.redis|}",
      "\t\ton_error:",
      "\t\t\tcontinue: false",
      "\n"
    ]
  },
  "create godspeed config": {
    "prefix": ["godspeed config"],
    "body": [
      "my_datasource:",
      "\tbase_url: MY_DATASOURCE_BASE_URL",
      "\tapi_key: MY_DATASOURCE_API_KEY",
      "\tapi_token: MY_DATASOURCE_API_TOKEN"
    ]
  },
  "kafka": {
    "prefix": ["config kafka"],
    "body": [
      "kafka:",
      "\tbrokers:",
      "\t\t__name: KAFKA_BROKERS",
      "\t\t__format: json",
      "\tclient_id: KAFKA_CLIENT_ID"
    ]
  },
  "log_level": {
    "prefix": ["log_level config"],
    "body": [
      "log_level: debug",
      "lang: coffee",
      "redact: []",
      "server_url: https://api.example.com:8443/v1/api",
      "httpbin:",
      "\t\tbase_url: https://httpbin.org",
      "request_body_limit: 50mb",
      "file_size_limit : 50mb"
    ]
  }
}
